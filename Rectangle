package Module4;


public class Rectangle extends Shape{
    //Constructor
    private double width;
    private double length;

    public Rectangle(double width, double length) {
        this.width = width;
        this.length = length;
    }

    @Override
    public double area() {
        return length * width;
    }
    
    @Override
    public double perimeter() {
        return (2* length) + (2*width);
    }
}

